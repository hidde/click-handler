; This file is for unifying the coding style for different editors and IDEs
; See editorconfig.org

; top-most EditorConfig file
root = true

[*]
charset = utf-8
trim_trailing_whitespace = true
end_of_line = lf
insert_final_newline = true
indent_style = tab
indent_size = 2

[*.yml]
indent_style = space
indent_size: = 2
